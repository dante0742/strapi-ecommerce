{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "Organize your content into categories"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "text"
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "mappedBy": "categories"
    },
    "parent_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "category"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "mappedBy": "parent_category"
    },
    "child_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "categories"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "child_category"
    }
  }
}
